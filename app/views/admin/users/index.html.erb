<table class="table table-bordered">
  <thead>
    <tr>
      <th>User ID</th>
      <th>User Name</th>
      <th>Delete</th>
      <th>Edit</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <% unless user.admin? %>
        <tr class="user-<%= user.id %>">
          <td><%= user.id %></td>
          <td><%= user.name %></td>
          <td><%= link_to "Delete", user_path(user), method: :delete %></td>
        </tr>
      <% end %>
    <% end %>
</tbody>
